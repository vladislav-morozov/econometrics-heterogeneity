	doi = {10.1093/restud/rdt011},
	file = {:D$\backslash$:/Academic Things/Econometrics/Articles/Applied/Production Function Estimation/Doraszelski, Jaumandreu (2013) Estimating Endogenous Productivity.pdf:pdf},
