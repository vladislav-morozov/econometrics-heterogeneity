	file = {:C$\backslash$:/Users/moren/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ledoit, Wolf - 2018 - Optimal Estimation of a Large-Dimensional Covariance Matrix Under Stein's Loss.pdf:pdf},
